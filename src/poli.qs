# poli
if $ARGS[0] = 'start':
	$sexloc = $CURLOC

	cla
	*clr
	clr
	hosprand = RAND(0,10)
	minut = minut + 5
	gs'stat'
	FColor = RGB(0,0,0)
	BColor = RGB(255,255,255)
	LColor = RGB(106,90,205)

	'<center><b><font color = maroon>Поликлиника</font></b></center>'
	'<center><img src="images/pic/poli.jpg"></center>'
	'Прием у врача стоит 1000 рублей.'
	'Прием у Психотерапевта стоит 1000 рублей.'
	'Прием у Венеролога стоит 1000 рублей.'
	''
	'Около кабинета венеролога висит объявление.'
	'Частная венерологическая клиника предоставляет следующие услуги.'
	'Полное излечение от молочницы 50 тысяч рублей.'
	'Полное излечение от герпеса 150 тысяч рублей.'
	'Полное излечение от сифилиса 350 тысяч рублей.'
	''
	'По вопросу госпитализации приходить в понедельник, до 10 числа каждого месяца'
	''
	'Аборт по желанию женщины можно делать до 84 дней беременности.'
	'Стоимость аборта 20 тысяч рублей.'
	'Предупреждаем, аборты опасны для здоровья. Всегда есть риск осложнений и не возможность в будущем забеременеть.'

	if workhosp = 1 and hour >= 8 and hour <= 9 and week < 6:act 'Выйти на смену':gt'poli','beginwork'

	act 'Выйти из Поликлиники':gt'street'

	if money >= 1000:
		if health < (vital*10 + stren*5):
			act 'Лечить здоровье (1000 руб)':
				cla
				money = money - 1000
				minut = minut + 60
				health = vital*10 + stren*5+1000
				if hosprand = 0:gt'poli','0'
				if hosprand = 1:gt'poli','1'
				if hosprand = 2:gt'poli','2'

				'Врач сделал вам укол и вам сразу же стало гораздо лучше'

				act 'Выйти':gt'poli','start'
			end
		end

		if willpower < intel*5 + will*5:
			act 'Идти к Психотерапевту (1000 руб)':
				cla
				money = money - 1000
				minut = minut + 60
				willpower = intel*5 + will*5+1000
				manna = (intel*magik) + magik*100 + vital*10 + rikudo
				if hosprand = 0:gt'poli','0'
				if hosprand = 1:gt'poli','1'
				if hosprand = 2:gt'poli','2'

				'Вы полежали на кушетке у психотерапевта и рассказали ему о своих невзгодах и вам сразу же стало гораздо легче на душе.'

				act 'Выйти':gt'poli','start'
			end
		end

		act 'Идти к Венерологу (1000 руб)':
			cls
			money = money - 1000
			minut = minut + 60
			gs'stat'

			'У вас взяли анализы и после небольшого ожидания врач пригласил вас в кабинет.'

			if venera > 0:'Врач состроил скорбную мину и сообщил, что у вас не все гладко с анализами.'

			if GerpesOnce = 1:
				'У вас обнаружен генитальный герпес. Это заболевание не излечимо и его можно только глушить на время.'

				if Gerpes >= 3:
					if GerpesNapr = 0:GerpesNapr = 3 & 'Вот вам направление на уколы от герпеса. Необходимо 3 укола, вы должны будете оплатить стоимость лекарств, это 1000 рублей.'
					if GerpesNapr > 0:'Сходите на уколы от герпеса, направление я вам выдал.'
				else
					'Герпес у вас в неактивной стадии, принимайте витамины и он себя не проявит.'
				end
			end

			if SifacOnce = 1:
				'У вас обнаружен сифилис. Это не излечимое, смертельное заболевание которое в наше время научились сдерживать, но не более того.'

				if Sifilis < 15:
					'Сейчас сифилис у вас в неактивной стадии. Поэтому смысла особого нет в терапии.'
				elseif Sifilis >= 15:
					if SifNapr = 0:SifNapr = 10 & 'Вот вам направление на уколы от сифилиса. Необходим курс из 10ти уколов, каждый укол будет стоить 5000 рублей.'
					if SifNapr > 0:'Сходите на уколы от сифилиса. Курс долгий, 10 дней. Но это необходимо, заболевание тяжелое.'
				end
			end

			if TriperOnce = 1:
				'У вас обнаружили гонорею. В принципе это заболевание уже излечимо.'

				if TriperNapr = 0:TriperNapr = 5 & 'Вот вам направление на уколы от гонореи. Необходимо 5ть уколов, каждый укол 1500 рублей'
			end

			if KandidozOnce = 1:
				'У вас обнаружен кандидоз. Или как ее называют в народе, молочница. Вещь не излечимая, но не очень опасная, к тому же легко глушится.'

				if KandidNapr = 0:KandidNapr = 1 & 'В аптеке можете купить таблетки.'
				if Kandidoz < 30:'Сейчас у вас кандидоз в неактивной стадии, принимайте витамины и он себя не проявит.'
			end

			if venera <= 0:'Врач вас обрадовал тем что у вас нет венерических заболеваний.'

			act 'Выйти':gt'poli','start'
		end

		if GerpesNapr > 0 and GerpesUkolDay ! day:
			act 'Идти на уколы от герпеса (1000 руб). Осталось <<GerpesNapr>> укола по направлению':
				cls
				money = money - 1000
				minut = minut + 60
				GerpesUkolDay = day
				GerpesNapr -= 1
				gs'stat'

				'Вам сделали очень болезненный укол в зад.'

				if GerpesNapr = 0:Gerpes = -10 & 'Это был последний укол'

				act 'Выйти':gt'poli','start'
			end
		end
	end

	if money >= 1500 and TriperNapr > 0 and TriperUkolDay ! day:
		act 'Идти на уколы от гонореи (1500 руб). Осталось <<TriperNapr>> укола по направлению':
			cls
			money = money - 1500
			minut = minut + 60
			TriperUkolDay = day
			TriperNapr -= 1
			gs'stat'

			'Вам сделали очень болезненный укол в зад.'
			if TriperNapr = 0:Triper = 0 & Venera -= 1 & TriperOnce = 0 & 'Это был последний укол'

			act 'Выйти':gt'poli','start'
		end
	end

	if money >= 5000 and SifNapr > 0 and SifUkolDay ! day:
		act 'Идти на уколы от сифилиса (5000 руб). Осталось <<SifNapr>> укола по направлению':
			cls
			money = money - 5000
			minut = minut + 60
			SifUkolDay = day
			SifNapr -= 1
			gs'stat'

			'Вам сделали очень болезненный укол в зад.'

			if SifNapr = 0:Sifilis = 0 & 'Это был последний укол'

			act 'Выйти':gt'poli','start'
		end
	end

	if money >= 20000 and preg = 1 and pregtime < 84:
		act 'Сделать аборт (20000 руб)':
			cla
			vidage = vidage + 1
			abort = abort + 1
			money = money - 20000
			pregtime = 0
			pregtalk = 0

			abortrand = RAND(0,10)
			if age > 18:
				if abortrand < 10:preg = 0
				if abortrand = 10:preg = 2
			elseif age < 18:
				if abortrand < 4:preg = 0
				if abortrand >= 5:preg = 2
			end

			'<center><img src="images/pic/abort.jpg"></center>'
			'Вас разложили на гинекологическом кресле, врач ввел вам во влагалище инструмент и по частям выдрал из вас плод.'

			act 'Выйти':gt'poli','start'
		end
	end

	if money >= 50000 and KandidozOnce = 1 and week = 1 and day <= 10:
		act 'Лечь в платную клинику для полного уничтожения вируса молочницы (50000 руб)':
			cla
			money = money - 50000
			day += 3
			week += 3
			KandidozOnce = 0
			Kandidoz = 0
			gs'stat'

			'Вы пролежали три дня в платной клинике и были полностью излечены от молочницы.'

			act 'Выйти':gt'poli','start'
		end
	end

	if money >= 150000 and GerpesOnce = 1 and week = 1 and day <= 10:
		act 'Лечь в платную клинику для полного уничтожения вируса герпеса (150000 руб)':
			cla
			money = money - 150000
			day += 3
			week += 3
			GerpesOnce = 0
			Gerpes = 0
			Venera -= 1
			gs'stat'

			'Вы пролежали три дня в платной клинике и были полностью излечены от герпеса.'

			act 'Выйти':gt'poli','start'
		end
	end

	if money >= 350000 and SifacOnce = 1 and week = 1 and day <= 10:
		act 'Лечь в платную клинику для полного уничтожения сифилиса (350000 руб)':
			cla
			money = money - 350000
			day += 3
			week += 3
			SifacOnce = 0
			Sifilis = 0
			Venera -= 1
			gs'stat'

			'Вы пролежали три дня в платной клинике и были полностью излечены от сифилиса.'

			act 'Выйти':gt'poli','start'
		end
	end

	if preg = 1 and pregtime = 280:
		act 'Родить':
			cla
			vidage = vidage + 1
			preg = 2
			pregtime = 0
			pregtalk = 0
			numkid += 1
			kid += 1
			daykid[numkid] = day
			monthkid[numkid] = month
			yearkid[numkid] = year

			$father[numkid] = $father

			haprikid[numkid] = RAND(0,3)
			glacol[numkid] = RAND(0,3)

			!'<center><img src="images/pic/abort.jpg"></center>'
			'Вас положили на койку и развели ноги. Началась страшная боль внизу живота, вы орали со всей дури и наконец то родили.'

			polkid[numkid] = RAND(0,1)
			if polkid[numkid] = 0:$polreb[numkid] = 'девочка' & 'Врач сказал что у вас родилась девочка.'
			if polkid[numkid] = 1:$polreb[numkid] = 'мальчик' & 'Врач сказал что у вас родился мальчик.'

			act 'Дать имя ребенку':
				$kidname[numkid] = input ("Как вы назовете ребенка?")

				if $kidname[numkid] <= 0:
					if polkid[numkid] = 0:$kidname = 'Маша'
					if polkid[numkid] = 1:$kidname = 'Миша'
				end

				act 'Выйти':gt'poli','start'
			end
		end
	end

	if sick > 0 and polisickday ! day:
		if ninelsex = 0:
			act 'Идти на прием к Терапевту':
				cla
				*clr
				polisickday = day

				terrnd = RAND(0,1)
				if terrnd = 0:gt'poli','drPP'
				if terrnd = 1:gt'poli','ninel'
			end
		elseif ninelsex > 0:
			act 'Идти на прием к Нинель':
				cla
				*clr
				polisickday = day
				gt'poli','ninel2'
			end
		end
	end

	if sick <= 0 and horny >= 90 and ninelsex >= 1 and ninelday ! day:
		act 'Искать Нинель':
			cla
			*clr
			ninelday = day

			ninelrand = RAND(0,2)
			if ninelrand = 0:
				'В регистратуре вам сказали, что Нинель в своем кабинете'

				act 'Идти к Нинель':gt'poli','ninel3'
			elseif ninelrand = 1:
				'В регистратуре вам сказали, что Нинель сегодня не принимает'

				act 'Уйти из поликлиники':gt'street'
			elseif ninelrand = 2:
				'В регистратуре вам сказали, что Нинель сегодня на вызовах'

				act 'Уйти из поликлиники':gt'street'
			end
		end
	end

	if katjob > 0:act 'Зайти к Главврачу':gt'poli','glavdoc'

	if zub > 0:
		act 'Идти к Стоматологу':
			cls
			zubpay = zub*30000
			gs'stat'

			'Стоматолог осмотрел вас и сказал, что протезирование будет стоить <<zubpay>> рублей'

			act 'Уйти из поликлиники':gt'street'

			if money >= zubpay:
				act 'Оплатить протезирование':
					cls
					money -= zubpay
					minut += 60
					zub = 0
					gs'stat'

					'Вы оплатили протезирование зубов и стоматолог в течении часа полностью привел в порядок ваши зубы.'

					act 'Уйти из поликлиники':gt'street'
				end
			end
		end
	end

	if anus > 5:
		act 'Идти к Проктологу':
			cls
			minut += 15
			if analplugIN ! 0:analplugIN = 0
			gs'stat'

			'<center><img src="images/picb/<<anpic>>.jpg"></center>'
			'Вы пришли в кабинет к проктологу. Он тут же попросил вас раздается ниже пояса и наклонится.'

			if anus <= 15:
				'Врач осмотрел ваш анус и сказал "Ну в принципе ничего страшного, немного растянутый анус, но никаких патологий."'
			elseif anus > 15 and anus <= 25:
				'Врач осмотрел ваш анус и сказал "У вас растянулся анус и сфинктер. Я бы рекомендовал процедуры, 1000 рублей за сеанс, это поможет анусу сократится до нормальных размеров."'
			elseif anus > 25 and anus <= 35:
				'Врач осмотрел ваш анус и сказал "У вас сильно растянут анус, скорее всего ваш сфинктер не способен удерживать кишечные газы. Я бы рекомендовал процедуры, 1000 рублей за сеанс, это поможет анусу сократится до нормальных размеров."'
			elseif anus > 35:
				'Врач осмотрел ваш анус и сказал "Боже мой! Вам надо осторожней обращаться со своей попой, у вас же вот вот выпадет кишка. Я бы рекомендовал процедуры, 1000 рублей за сеанс, это поможет анусу сократится до нормальных размеров."'
			end

			if money >= 1000 and anus > 15 and proktologday ! day:
				act 'Оплатить процедуру':
					cls
					money -= 1000
					minut += 60
					proktologday = day
					anus -= 5
					gs'stat'

					'<center><img src="images/picb/<<anpic>>.jpg"></center>'
					'Вы оплатили процедуру и врач целый час ковырялся в вашей жопе приводя ее в порядок.'

					act 'Уйти из поликлиники':gt'street'
				end
			end

			act 'Уйти из поликлиники':gt'street'
		end
	end

	if glass > 0:
		act 'Идти к Окулисту':
			cls
			gs'stat'

			'Окулист осмотрел вас и сказал что вам можно сделать лазерную коррекцию зрения и стоит эта процедура 100 тысяч. Но от чтения книг зрение опять может испортится.'

			if glass = 1:'Врач глядя на вас добавляет, конечно вы можете купить очки которые будут лучше смотреться на вас. 8 тысяч.'

			act 'Уйти из поликлиники':gt'street'

			if money >= 8000 and glass = 1:
				act 'Купить очки':
					cls
					money -= 8000
					minut += 60
					glass = 2
					gs'stat'

					'Вы купили новые, более стильные очки.'

					act 'Уйти из поликлиники':gt'street'
				end
			end

			if money >= 100000 and glass ! 0:
				act 'Оплатить лазерную коррекцию':
					cls
					money -= 100000
					minut += 60
					blizoruk = 0
					glass = 0
					gs'stat'

					'Вы оплатили лазерную коррекцию и вам за час исправили зрение.'

					act 'Уйти из поликлиники':gt'street'
				end
			end
		end
	end
end

if $ARGS[0] = 'drPP':
	cla
	clr
	*clr
	minut = minut + RAND(3,10)
	gs'stat'

	'Вы сидите в коридоре среди кашляющих и охающих старушек в ожидании своей очереди.'
	'Наконец, очередь подошла'

	act 'Войти в кабинет':
		cla
		*clr

		'<center><img src="images/add-on/drPP.jpg"></center>'

		if drPP = 0:'За столом сидит пожилой лысоватый мужчина благообразной наружности. "Здравствуйте, больная. Меня зовут Петр Петрович, я ваш участковый терапевт, - говорит он, разглядывая вас сквозь очки".'
		if drPP = 1:'За столом сиди Петр Петрович. Он улыбается вам и говорит: "Здравствуйте, больная.'
		drPP = 1

		'(Вы) - Здравствуйте, доктор.'
		'(Доктор) - На что жалуетесь, больная?'

		act 'Жаловаться':
			cla
			*clr
			minut = minut + 5

			'(Вы) - Ох, доктор, похоже я простудилась - нос заложило, горло побаливает... И кашель.'
			'(Доктор) - Так-с... Температура есть?'
			'(Вы) - Да, небольшая.'
			'(Доктор) - Ну-с, давайте вас послушаем'

			act 'Выполнять указания врача':
				cla
				minut = minut + 20

				'<center><img src="images/inBed/ninel03"></center>'
				'Доктор внимательно прослушал ваши легкие, заглянул в горло, дал градусник - измерить температуру и, наконец, заключил:'
				'(Доктор) - Ничего страшного - обыкновенная простуда - ОРВИ. Купите в аптеке вот эти лекарства, полежите пару дней в постельке и все будет в порядке. Главное - не переохлаждаться.'
				'(Вы) - Спасибо, доктор. До свидания.'

				act 'Идти домой':gt'street'
			end
		end
	end
end

if $ARGS[0] = 'ninel':
	cla
	clr
	*clr
	minut = minut + RAND(3,15)
	gs'stat'

	'Вы сидите в коридоре среди кашляющих и охающих старушек в ожидании своей очереди.'
	'Наконец, очередь подошла'

	act 'Войти в кабинет':
